page {
  background-color: #fff;
}

.bottom {
  position: absolute;
  bottom: 40rpx;
  width: 100%;
  text-align: center;
  display: inline-block;
  color: #ccc;
}

.top {
  height: 500rpx;
  background-image: linear-gradient(45deg, #135FAC 1%, #1e88e5 64%, #40BAF5 97%);
}

.contents {
  width: 90%;
  margin-left: 5%;
  border-radius: 12rpx;
  background-color: #fff;
  margin-top: -200rpx;
  padding: 0 30rpx;
  box-shadow: 6rpx 10rpx 6rpx rgba(0, 0, 0, 0.1);
  height: 180px;
}

.user_name {
  line-height: 1.6em;
  flex: 1;
  font-size: 30rpx;
  display: inline-flex;
  border-bottom: 1px solid #dbdbdb;
  border-top: 0px;
  border-left: 0px;
  border-right: 0px;
}

.content-text {
  min-height: 100rpx;
  position: relative;
  display: flex;
  align-items: center;
  width: 80%;
  padding-left: 100rpx;
  padding-top: 100rpx;
}

.login-btn {
  width: 300rpx;
  margin-top: 100rpx;
}
.register-btn{
  padding-top: 20rpx;
}
.cus-pon {
  cursor: pointer;
}
.iconSty {
  position: relative;
  top: 2px;
}
.logo {
  width: 50px;
  height: 50px;
}